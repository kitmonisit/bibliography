@article{joseph05,
	Author = {Joseph, A.D.},
	Date-Added = {2012-12-18 10:53:13 +0800},
	Date-Modified = {2012-12-18 10:53:27 +0800},
	Doi = {10.1109/MPRV.2005.8},
